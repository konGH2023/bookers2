<h1>Books#edit</h1>
<h1>Editing Book</h1>



<%= form_with model: @book, url: book_path(@book.id) , method: :patch do |f| %>
<!-- update_book_path(@books) -->
  <h4>Title</h4>
  <%= f.text_field :title %>

  <h4>Body</h4>
  <%= f.text_area :body %>

  <%= f.submit "Update Book" %>

<%= link_to "Show", book_path(@book) %>
<%= link_to "Back", books_path %>
<% end %>
